  "scripts": {
    "start": "ng serve --proxy-config proxy.conf.json --aot true",
    "start:jit": "ng serve --proxy-config proxy.conf.json",
    "start:prod": "ng serve --proxy-config proxy.conf.json --prod",
    "build": "ng build --aot true",
    "deployment": "ng build --prod",
    "lint": "tslint \"src/**/*.ts\"",
    "test": "ng test",
    "mvnTest": "ng test --browsers PhantomJS --watch=false",
    "test:chrome": "ng test --browsers Chrome --watch=false",
    "test:chrome:headless": "ng test --browsers ChromeHeadless --watch=false",
    "test:firefox": "ng test --browsers Firefox --watch=false",
    "pree2e": "webdriver-manager update",
    "e2e": "cd e2e && protractor protractor.conf.js",
    "jenkinsE2E": "npm run pree2e && protractor e2e/protractor.jenkins.conf.js",
    "build:backend": "cd .. && cd .. && cd 01.Backend && mvn clean docker:stop install -DskipTests -Pprod && cd .. && docker build -t macoco_tomee .",
    "start:backend": "cd .. && cd .. && cd 01.Backend && mvn clean docker:stop install docker:start tomee:run docker:stop -DskipTests",
    "socat-fix": "socat -d TCP-LISTEN:2375,range=127.0.0.1/32,reuseaddr,fork UNIX:/var/run/docker.sock",
    "compodoc": "./node_modules/.bin/compodoc -p tsconfig.json -s -r 4201",
    "createDummy": "../../Deployment/Tools/createDummy.sh",
    "sass-fix": "npm rebuild node-sass"
  },
