{
  "compileOnSave": false,
  "compilerOptions": {
    "outDir": "./dist/out-tsc",
    "sourceMap": true,
    "declaration": false,
    "strict": false,
    "baseUrl": "./",
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "noStrictGenericChecks": true,
    "target": "es5",
    "typeRoots": [
      "node_modules/@types"
    ],
    "lib": [
      "es2017",
      "dom"
    ],
    "paths": {
      // Common
      "@shared/*": ["src/app/shared/*"],
      "@targetgui/*" : ["target/generated-sources/gui/*"],
      "@targetdtos/*" : ["target/generated-sources/dtos/*"],
      "@srcdtos/*" : ["src/app/shared/architecture/data/dtos/*"],
      "@targetrte/*": ["target/generated-sources/rte/*"],
      "@components/*": ["src/app/shared/components/guidsl/*"],
      "@utils/*": ["src/app/utils/*"],
      "@navigation/*": ["target/generated-sources/navigation/navigation/*"],

      // Test
      "@testutils/*": ["src/tests/utils/*"],

      // Services
      "@services/*": ["src/app/shared/architecture/services/*"],
      "@jsonapiservice/*": ["src/app/shared/architecture/services/*"],
      "@loggerservice/*": ["src/app/shared/architecture/services/*"],

      // Commands
      "@commands/*": ["target/generated-sources/commands/*"],

      // Dashboard
      "@dashboard/*": ["src/app/dashboard/*"],

      // Einstellungen
      "@einstellungenrollentabelleentry-dto/*" : ["target/generated-sources/dtos/*"],
      "@einstellungenbenutzertabelleentry-dto/*": ["target/generated-sources/dtos/*"],
      "@einstellungenbenutzerformvalidator/*": ["target/generated-sources/dtos/*"],
      "@einstellungenbenutzertabelle-dto/*": ["target/generated-sources/dtos/*"],
      "@einstellungenrollentabelle-dto/*": ["target/generated-sources/dtos/*"],
      "@meinbenutzerpasswortzuruecksetzen-dto/*" : ["target/generated-sources/dtos/*"],
      "@einstellungenbenutzerform/*" : ["src/app/einstellungen/*"],
      "@meinbenutzerinfotabelle-dto/*": ["target/generated-sources/dtos/*"],
      "@meinbenutzerrollentabelle-dto/*": ["target/generated-sources/dtos/*"],
      "@userlist-dto/*" : ["target/generated-sources/dtos/*"],
      "@rolelist-dto/*" : ["target/generated-sources/dtos/*"],
      "@statuslist-dto/*" : ["target/generated-sources/dtos/*"],
      "@objectlist-dto/*" : ["target/generated-sources/dtos/*"],
      "@linegraph-dto/*" : ["target/generated-sources/dtos/*"],

      // Beispielkomponenten
      "@beispielepiechart-dto/*" : ["target/generated-sources/dtos/*"],
      "@datumsbereich-dto/*" : ["target/generated-sources/dtos/*"],
      "@beispielebarchart-dto/*" : ["target/generated-sources/dtos/*"],
      "@beispielehorizontalbarchart-dto/*" : ["target/generated-sources/dtos/*"],
      "@editabletableexample-dto/*" : ["target/generated-sources/dtos/*"],
      "@editabletableexampleentry-dto/*" : ["target/generated-sources/dtos/*"],
      "@rechnungsstellungstatus-dto/*" : ["target/generated-sources/dtos/*"],
      "@beispielelinechart-dto/*" : ["target/generated-sources/dtos/*"],
      "@tempsensortable-dto/*" : ["target/generated-sources/dtos/*"],
      "@alarmctrltable-dto/*" : ["target/generated-sources/dtos/*"],
      "@sirenctrltable-dto/*" : ["target/generated-sources/dtos/*"],
      "@densitysensordata-dto/*": ["target/generated-sources/dtos/*"],
      "@smokesensortable-dto/*" : ["target/generated-sources/dtos/*"],
      "@sirenctrl-dto/*": ["target/generated-sources/dtos/*"]
    }
  }
}