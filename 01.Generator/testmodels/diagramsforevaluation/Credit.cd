package testmodels.diagramsforevaluation;

classdiagram Credit{

  class Bank{
    String bic;
  }

  class BankAccount{
    String iban;
  }

  class Creditor extends FinancialEntity{
    String ics;
    String address_line_1;
    String address_line_2;
  }

  class Debtor extends FinancialEntity{
    String rum;
    Date mandate_signature_date;
  }

  class FinancialEntity{
    String name;
  }

  class Payment{
    String payment_information_id;
    Date requested_collection_date;
  }

  class Transaction{
    String eeid;
    Float amount;
  }

  association Bank_BankAccount [1] BankAccount (bankAccount) <-> (bank) Bank [1] ;

  association FinancialEntity_BankAccount [1] BankAccount (bankAccount) <- (financialEntity) FinancialEntity [1] ;

  association Debtor_Transaction [1] Transaction (transaction) -> (debtor) Debtor [1] ;

  association Creditor_Transaction [1] Transaction (transaction) -> (creditor) Creditor [1] ;

  association Payment_Transaction [1] Transaction (transaction) <- (payment) Payment [1] ;

}
