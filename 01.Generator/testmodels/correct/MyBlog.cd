package testmodels.correct;

classdiagram MyBlog {
  class Blog {
    String name;
    List<String> tags;
  }

  class Member{
    String name;
    int age;
    String alias;
    String email;
    Optional<String> a;
    Optional<Boolean> b;
    Optional<Post> c; // TODO Fix this bug
  }

  abstract class Post{
    String title;
    List<String> tags;
    String content;
    Optional<Post> parentPost;
  }

  enum MediaType{
    DOCUMENT,
    PICTURE,
    LINK,
    MOVIE;
  }

  class MediaPost extends Post{
    String category;
    MediaType mediaType;
  }

  class MessagePost extends Post{
  }

  association [*] Blog -> Member [*];
  association [1] Member <-> Post [*];
}
