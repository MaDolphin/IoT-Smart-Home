/********************************************************
 * This CD causes current bugs in DEx. Most bugs
 * occur at the swing GUI due to compiliation errors.
 *******************************************************/
classdiagram Bugged {

  /////////////////////////////////
  // Category: Violated CoCos    //
  /////////////////////////////////
  
  class ViolatedCocos {
    // The following two attributes are violating the cocos. All other types with predefined assigned values are passing the Cocos properly.
    
    // byte byteAttr = 0; // Bugged in DEx: 0xC4A11 The value assignment for the attribute byteAttr in class AttributeTestValues is not compatible to its type byte.
    // short shortAttr = 130; // Bugged in DEx: 0xC4A11 The value assignment for the attribute byteAttr in class AttributeTestValues is not compatible to its type byte.
  }
  
  /////////////////////////////////
  // Category: Compile Errors    //
  /////////////////////////////////
  
  class SomeCompileErrors {
    /Date dateAttr; // Derived date. Should work, there is a default value for dates,
                    // but the DEx Swing GUI produces a compile Bug at derived dates!
                    
    List<Date> dates; // Causes compile errors in persistence and gui
    Set<Date> dates2; // Also with Set<Date>
  }
  
  // Qualified associations (tested with named qualifiers)
  class NQualifiedAssocs { }
  class QualifiedAssocTarget {
    Date dateAttr;
    boolean boolAttr; // These 3 attributes as name qualifiers for a qualified assoc cause problems
    char charAttr;   
  }
  association qna2 NQualifiedAssocs [[boolAttr]] -> QualifiedAssocTarget [1];
  association qna3 NQualifiedAssocs [[dateAttr]] -> QualifiedAssocTarget [1];
  association qna10 NQualifiedAssocs [[charAttr]] -> QualifiedAssocTarget [1]; // as well as for all other 
                                                                               // cardinalites *, 1..*, 0..1
                                                                               // and type qualified assocs
                                                                               
  
  

  /////////////////////////////////
  // Category: Serverside bugs:  //
  /////////////////////////////////
  // Bug: Creating A results in ASuper not beeing visible... known bug
  //      There is a similiar bug for interfaces that is known by Alexander.
  abstract class ASuper {
    String abstractAttr;
  }
  
  class A extends ASuper {
    String concreteAttr;
  }
}
