// Ticket #532(fixed), 587

package useimported;  // DR Bib

import imported.Types.*;

classdiagram Bib {

  <<user>>
  class Person {
    // leave username and password attributes in place
    + String userName;
    + Password password; 
    
    <<inList>> + String firstName;
    <<inList>> + String surName;
    + Email email;
    + String room; 
  }

  class Book {
    <<inList>> + String title;
    + Text authors; 
    + String publisher;
    + Integer publicationyear;
    + Float number;
  }

  /*
    leave this class in place, it will serve as entity for group association
  */
  class GroupInternal {
    <<inList>> + String groupName;
    + String groupDesc;
  }

  /*
    leave this in place and potentially replace Person by the classname of your user class
  */
  association [*] Person (users) <-> (groups) GroupInternal [*];
  association [1] Person (lender) <-> (books) Book [*];

}
